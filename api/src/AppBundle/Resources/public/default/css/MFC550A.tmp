/* Global Setting */
body{margin:0; padding:0;text-align:left; font-size:12px; font-family:'黑体',Arial; background:#000;}
table{border-collapse:collapse;}
td,th{padding:0;}
ul,li,dd,dl,dt{margin:0; padding:0; list-style:none; word-break:break-all;}
img{border:0;}
p{margin:0; word-break:break-all;}
h1,h2,h3,h4{margin:0;padding:0; font-weight:normal; font-size:14px;}
em{ font-size:12px; font-style:normal;}
hr{background:none;border:none;border-bottom:1px solid #CCC;#border-bottom:0;}
.clear {clear:both;height:0;font-size:1px;line-height: 0px;}
a{text-decoration:none; border:0; outline:none; blr:expression(this.onFocus=this.blur());}
select,input,textarea{font-family:inherit; outline:none;}

.wrapper{ width:1000px; margin:0 auto; border:none; padding:0;}
.innerDiv{ width:100%; height:100%; position:relative;}
.abs{ position:absolute;}
.outDiv{ width:640px; margin:0 auto; overflow:hidden;}

.page{ width:640px; height:1039px; margin:0 0 0 -320px; position:absolute; top:0; left:50%;}

.indexImg1{ left:30px; top:150px;}
.indexImg2{ left:90px; top:540px;}
.indexImg3{ left:215px; top:765px;}
.btnLine{ text-align:center; width:100%; position:absolute; left:0; top:920px;}
.btnLine2{ text-align:center; width:100%; position:absolute; left:0; top:940px;}
.btnLine3{ text-align:center; width:100%; position:absolute; left:0; top:960px;}
.logo{ position:absolute; left:10px; top:10px;}

.indexImg4{ left:160px; top:150px;}
.selBtn1{ left:0; top:270px;}
.selBtn2{ left:0; top:448px;}
.selBtn3{ left:0; top:672px;}
.selImg1{ left:47px; top:0;}
.selImg2{ left:388px; top:0;}
.selImg3{ left:77px; top:0;}
.on .selImg{
-webkit-animation:c1Act 2s 0s linear both infinite;
}
@-webkit-keyframes c1Act{
0%{-webkit-transform:rotate(0);}
5%{-webkit-transform:rotate(10deg);}
15%{-webkit-transform:rotate(-10deg);}
25%{-webkit-transform:rotate(10deg);}
35%{-webkit-transform:rotate(-10deg);}
40%{-webkit-transform:rotate(0);}
100%{-webkit-transform:rotate(0);}
}

